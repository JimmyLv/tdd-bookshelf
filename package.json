{
  "name": "cd-bookshelf-frontend",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "test": "jest src",
    "lint": "eslint src",
    "cy:open": "cypress open",
    "cy:run": "cypress run --spec 'cypress/integration/E2E/**/*'"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "cypress": "^3.8.0",
    "lodash": "^4.17.15",
    "react-copy-to-clipboard": "^5.0.1",
    "react-motion": "^0.5.2",
    "react-transition-group": "^2.5.0",
    "reselect": "^3.0.1",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@testing-library/cypress": "^5.0.2",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.25",
    "antd-pro-server": "^1.0.1",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.4.0",
    "eslint-config-umi": "^0.1.5",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^7.2.2",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^1.19.1",
    "readline": "^1.3.0",
    "redux-saga-test-plan": "^3.7.0",
    "umi": "^2.2.7",
    "umi-plugin-react": "^1.1.1"
  },
  "peerDependencies": {
    "antd": "^3.26.4",
    "antd-mobile": "^2.3.1",
    "dva": "^2.4.1",
    "react": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux-saga": "^0.16.2"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test && npm run lint"
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "babel": {
    "presets": [
      [
        "babel-preset-umi",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ]
    ],
    "plugins": [
      [
        "@babel/transform-runtime"
      ]
    ]
  }
}
